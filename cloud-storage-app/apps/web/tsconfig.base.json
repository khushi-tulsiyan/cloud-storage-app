{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "strict": true,
      "moduleResolution": "node",
      "baseUrl": ".",
      "paths": {
        "*": ["types/*"]
      },
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    }
  }
  